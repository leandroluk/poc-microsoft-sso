{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "dev"
      ],
      "outputCapture": "std",
      "runtimeExecutable": "npm",
      "type": "node"
    },
    {
      "name": "file",
      "program": "${file}",
      "request": "launch",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    }
  ],
  "compounds": []
}